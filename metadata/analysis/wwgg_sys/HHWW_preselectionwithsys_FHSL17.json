{
    "name": "diphoton_preselection",
    "function": {
        "module_name": "higgs_dna.analysis",
        "function_name": "run_analysis"
    },
    "tag_sequence": [
        {
            "module_name": "higgs_dna.taggers.diphoton_tagger",
            "tagger": "DiphotonTagger",
            "kwargs": {
                "name": "diphoton_tagger",
                "options": {
                    "gen_info": {
                        "calculate": false
                    }
                }
            }
        },
        {
            "module_name": "higgs_dna.taggers.HHWW_Tagger_combined_FHSL",
            "tagger": "HHWW_Preselection_FHSL",
            "kwargs": {
                "name": "HHWW_Tagger_combined_FHSL",
                "options": {
                    "gen_info": {
                        "is_Signal": true
                    }
                }
            }
        }
    ],
    "systematics" : {
        "weights" : {
            "electron_veto_sf" : {
                "type" : "object",
                "method" : "from_function",
                "function" : {
                    "module_name" : "higgs_dna.systematics.photon_systematics",
                    "name" : "photon_electron_veto_sf"
                 },
                 "modify_central_weight" : true,
                 "input_collection" : "Photon",
                 "target_collections" : [["Diphoton", "Photon"]]
            },
            "photon_id_sf" : {
                "type" : "object",
                "method" : "from_function",
                "function" : {
                    "module_name" : "higgs_dna.systematics.photon_systematics",
                    "name" : "photon_mva_id_sf"
                },
                "modify_central_weight" : true,
                "input_collection" : "Photon",
                "target_collections" : [["Diphoton", "Photon"]],
                "kwargs" : {
                    "working_point" : "wp90"
                }
            },
            "photon_presel_sf" : {
                "type" : "object",
                "method" : "from_function",
                "function" : {
                    "module_name" : "higgs_dna.systematics.photon_systematics",
                    "name" : "photon_presel_sf_lowmass"
                },
                "modify_central_weight" : true,
                "input_collection" : "Photon",
                "target_collections" : [["Diphoton", "Photon"]] 
            },
            "nonisoelectron_id_sf" : {
                "type" : "object",
                "method" : "from_function",
                "function" : {
                    "module_name" : "higgs_dna.systematics.lepton_systematics",
                    "name" : "electron_id_sf"
                },
                "modify_central_weight" : true,
                "input_collection" : "Electron",
                "target_collections" : ["SelectedElectron_noiso"],
                "kwargs" : {
                    "working_point" : "wp90noiso"
                }
            },
            "isoelectron_id_sf" : {
                "type" : "object",
                "method" : "from_function",
                "function" : {
                    "module_name" : "higgs_dna.systematics.lepton_systematics",
                    "name" : "electron_id_sf"
                },
                "modify_central_weight" : true,
                "input_collection" : "Electron",
                "target_collections" : ["SelectedElectron_iso","SelectedElectron_noiso"],
                "kwargs" : {
                    "working_point" : "wp80iso"
                }
                
            },
            "isomuon_id_sf" : {
                "type" : "object",
                "method" : "from_function",
                "function" : {
                    "module_name" : "higgs_dna.systematics.lepton_systematics",
                    "name" : "muon_tightid_tightiso_sf"
                },
                "modify_central_weight" : true,
                "input_collection" : "Muon",
                "target_collections" : ["SelectedMuon_iso"]
                
            },
            "nonisomuon_highptid_sf" : {
                "type" : "object",
                "method" : "from_function",
                "function" : {
                    "module_name" : "higgs_dna.systematics.lepton_systematics",
                    "name" : "highptmuonsf"
                },
                "modify_central_weight" : true,
                "input_collection" : "Muon",
                "target_collections" : ["SelectedMuon_noiso"]
                
            },
            "pu_reweight_sf" : {
                "type" : "event",
                "method" : "from_function",
                "function" : {
                    "module_name" : "higgs_dna.systematics.pileup_systematics",
                    "name" : "pu_reweight_sf"
                },
                "modify_central_weight" : true,
                "requires_branches" : ["Pileup_nTrueInt"]
                
            },
            "L1_prefiring_sf" : {
                "type" : "event",
                "method" : "from_branch",
                "branches" : {
                    "central" : "PrefireWeight",
                    "up" : "PrefireWeight_Up",
                    "down" : "PrefireWeight_Down"
                },
                "modify_central_weight" : true            },
            "trigger_sf" : {
                "type" : "event",
                "method" : "from_function",
                "function" : {
                    "module_name" : "higgs_dna.systematics.photon_systematics",
                    "name" : "trigger_lowmass_sf"
                },
                "modify_central_weight" : true,
                "requires_branches" : ["Diphoton", "LeadPhoton", "SubleadPhoton"]
            },
            "lhe_scale0_sf" : {
                "type" : "event",
                "method" : "from_branch",
                "branches" : {
                    "central" : "LHEScaleWeight_Four",
                    "up" 			: "LHEScaleWeight_Five",
                    "down" 		: "LHEScaleWeight_Three"
                },
                "modify_central_weight" : true
            }, 
            "lhe_scale1_sf" : {
                "type" : "event",
                "method" : "from_branch",
                "branches" : {
                    "central" : "LHEScaleWeight_Four",
                    "up" 			: "LHEScaleWeight_Seven",
                    "down" 		: "LHEScaleWeight_One"
                },
                "modify_central_weight" : true
            }, 
            "lhe_scale2_sf" : {
                "type" : "event",
                "method" : "from_branch",
                "branches" : {
                    "central" : "LHEScaleWeight_Four",
                    "up" 			: "LHEScaleWeight_Eight",
                    "down" 		: "LHEScaleWeight_Zero"
                },
                "modify_central_weight" : true
            }
    },
    "independent_collections" : {
        "fnuf" : {
            "method" : "from_function",
            "branch_modified" : ["Photon", "pt"],
            "function" : {
                "module_name" : "higgs_dna.systematics.photon_systematics",
                "name" : "fnuf_unc"
            }
        },
        "material" : {
            "method" : "from_function",
            "branch_modified" : ["Photon", "pt"],
            "function" : {
                "module_name" : "higgs_dna.systematics.photon_systematics",
                "name" : "material_unc"
            }
        },
        "scale" : {
            "method" : "from_branch",
            "branch_modified" : ["Photon", "pt"],
            "branches" : {
                "up" : ["Photon", "pt_ScaleUp"],
                "down" : ["Photon", "pt_ScaleDown"]
            }
        },
        "smear" : {
            "method" : "from_branch",
            "branch_modified" : ["Photon", "pt"],
            "branches" : {
                "up" : ["Photon", "dEsigmaUp"],
                "down" : ["Photon", "dEsigmaDown"]
            },
            "additive" : true
        }, 
        "JER" : {
            "method" : "from_branch",
            "branch_modified" : ["Jet", "pt"],
            "branches" : {
                "up" : ["Jet", "pt_jerUp"],
                "down" : ["Jet", "pt_jerDown"]
            }
        },
        "JES" : {
            "method" : "from_branch",
            "branch_modified" : ["Jet", "pt"],
            "branches" : {
                "up" : ["Jet", "pt_jesTotalUp"],
                "down" : ["Jet", "pt_jesTotalDown"]
            }
        },
        "FJES" :{
            "method" : "from_branch",
            "branch_modified" : ["FatJet", "pt"],
            "branches" : {
                "up" : ["FatJet", "pt_jesTotalUp"],
                "down" : ["FatJet", "pt_jesTotalDown"]
            }
        },
        "FJER" : {
            "method" : "from_branch",
            "branch_modified" : ["FatJet", "pt"],
            "branches" : {
                "up" : ["FatJet", "pt_jerUp"],
                "down" : ["FatJet", "pt_jerDown"]
            }},
        "MET_JES" : {
            "method" : "from_branch",
            "branch_modified" : "MET_pt",
            "branches" : {
                "up" : "MET_T1_pt_jesTotalUp",
                "down" : "MET_T1_pt_jesTotalDown"
            }
        },
        "MET_Unclustered" : {
            "method" : "from_branch",
            "branch_modified" : "MET_pt",
            "branches" : {
                "up" : "MET_T1_pt_unclustEnUp",
                "down" : "MET_T1_pt_unclustEnDown"
            }
        },
        "Muon_pt" : {
                "method" : "from_branch",
                "branch_modified" : ["Muon", "pt"],
                "branches" : {
                    "up" : ["Muon", "correctedUp_pt"],
                    "down" : ["Muon", "correctedDown_pt"]
                }
            }}},

