    },
    "tag_sequence": [
        {
            "module_name": "higgs_dna.taggers.diphoton_tagger",
            "tagger": "DiphotonTagger",
            "kwargs": {
                "name": "diphoton_tagger",
                "options": {
                    "gen_info": {
                        "calculate": false
                    }
                }
            }
        },
        {
            "module_name": "higgs_dna.taggers.HHWW_Tagger_bbgg_zzgg",
            "tagger": "HHWW_Tagger_combinedYH_FHSL",
            "kwargs": {
                "name": "HHWW_Tagger_combinedYH_FHSL",
                "options": {
                    "gen_info": {
                        "is_Signal": false
                    }
                }
            }
        }
    ],
    "systematics" : {
        "weights" : {
            "electron_veto_sf" : {
                "type" : "object",
                "method" : "from_function",
                "function" : {
                    "module_name" : "higgs_dna.systematics.photon_systematics",
                    "name" : "photon_electron_veto_sf"
                 },
                 "modify_central_weight" : false,
                 "input_collection" : "Photon",
                 "target_collections" : [["Diphoton", "Photon"]]
            },
            "photon_id_sf" : {
                "type" : "object",
